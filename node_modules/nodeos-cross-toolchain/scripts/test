#!/usr/bin/env bash


TOOLS=`pwd`/out

source scripts/adjustEnvVars.sh || exit $?


LIBC="/lib/ld-musl-$CPU_PORT.so.1"

(
  cd $OBJECTS

  echo 'main(){}' > dummy.c        || exit 1
  $TARGET-gcc dummy.c              || exit 2
  readelf -l a.out | grep -q $LIBC || exit 3
  rm dummy.c a.out                 || exit 4
) || exit $?
